<% extend PeopleBlockHelper %>

<%= block_title(block.view_title) %>

<div>
  <% unless block.profile_count == 0 %>
    <ul>
      <%= profiles_images_list(block.profile_list) %>
    </ul>
  <% else %>
    <div class="common-profile-list-block-none"><%= c_('None') %></div>
  <% end %>
  <br style="clear:both" />
</div>
