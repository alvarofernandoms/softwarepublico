### Colab proxied apps
PROXIED_APPS:
  gitlab:
    upstream: 'http://<%= node['peers']['integration'] %>:81/gitlab/'
    private_token: '<%= @get_gitlab_private_token.call %>'
  noosfero:
    upstream: 'http://<%= node['peers']['social'] %>:80/social/'
    private_token: '<%= @get_noosfero_private_token.call %>'
